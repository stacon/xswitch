{
  "name": "@stacon/xswitch",
  "version": "1.0.1",
  "description": "An expressive functional switch",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm-run-all clean:dist transpile",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "transpile": "babel ./src --out-dir ./dist",
    "clean:dist": "rimraf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacon/xswitch.git"
  },
  "keywords": [
    "switch",
    "javascript",
    "fp",
    "functonal"
  ],
  "author": "Constantinos Stathis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stacon/xswitch/issues"
  },
  "homepage": "https://github.com/stacon/xswitch#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "babel": "^6.23.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "!**/index.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/coverage/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/coverage/"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "node"
    ]
  }
}
